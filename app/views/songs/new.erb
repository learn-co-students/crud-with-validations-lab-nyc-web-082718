<% if flash[:error] %>
  <% flash[:error].each do |error| %>
    <%= error %>
  <% end %>
<% end %>

<%= form_for @song do |f| %>
  Title: <%= f.text_field :title %>
  Released: <%= f.text_field :released %>
  Year: <%= f.text_field :release_year %>
  Artist: <%= f.text_field :artist_name %>
  Genre: <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
